    "url": "https://github.com/onCondition/onCondition-server/issues"
  },
  "dependencies": {
    "aws-sdk": "^2.983.0",
    "axios": "^0.21.4",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "firebase-admin": "^9.11.1",
    "http-errors": "~1.6.3",
    "mongodb": "^4.1.1",
    "mongoose": "^6.0.4",
    "mongoose-findorcreate": "^3.0.0",
    "mongoose-paginate-v2": "^1.4.2",
    "morgan": "~1.9.1",
    "multer": "^1.4.3",
    "multer-s3": "^2.9.0"
  },
  "devDependencies": {
    "eslint": "^7.30.0",
