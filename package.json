  "scripts": {
    "start": "node ./bin/www",
    "dev": "SET NODE_ENV=development & nodemon ./bin/www"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onCondition/onCondition-server.git"
  },
  "author": "",
  "license": "ISC",
